<!DOCTYPE html>
<html>
  <head>
    <%- include ('partial/header') %>
  </head>
  <body>
    <%- include ('partial/navbar') %>
    <div class='container mt-4'>
      <h1>Your profile</h1>
      <form action="" class="mt-4">
        <div class="form-group">
          <label for="" class=""><b>Name</b></label>
          <input type="text" class="form-control-plaintext" name="name" value="<%= name %>" readonly>
        </div>
        <div class="form-group">
          <label for="" class=""><b>DOB</b></label>
          <input type="text" class="form-control-plaintext" name="dob" value="<%= dob %>" readonly>
        </div>
        <div class="form-group">
          <label for="" class=""><b>About</b></label> 
          <input type="text" class="form-control-plaintext" name="about" value="<%= about %>" readonly>
        </div>
        <div class="form-group">
          <label for="" class=""><b>Status</b></label>
          <input type="text" class="form-control-plaintext" name="status" value="<%= status %>" readonly>
        </div>
      </form>
      <form action="/dogs/you/edit" method='GET'>
        <button type='Submit' class="btn btn-primary">Edit Profile</button>
      </form>
    </div>

    <%- include ('partial/requiredjs') %>

  </body>
</html>